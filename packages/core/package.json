{
  "name": "@qualweb/core",
  "version": "0.7.0-alpha",
  "description": "QualWeb evaluator core engine",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --require esm",
    "test:core": "mocha --require esm test/core.spec.js",
    "test:html": "mocha --require esm test/html.spec.js",
    "test:crawler": "mocha --require esm test/crawler.spec.js",
    "test:file": "mocha --require esm test/file.spec.js",
    "test:urls": "mocha --require esm test/urls.spec.js",
    "test:parallel": "mocha --require esm test/parallel.spec.js",
    "test:validator": "mocha --require esm test/validator.spec.js",
    "test:wp": "mocha --require esm test/wappalyzer.spec.js",
    "test:locale": "mocha --require esm test/locale.spec.js",
    "prebuild": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "npm run format && npm run lint && npm run prebuild && tsc --build",
    "docs": "typedoc --out docs src/index.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "qualweb",
    "evaluator",
    "web",
    "accessibility",
    "a11y"
  ],
  "homepage": "https://github.com/qualweb/core#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/qualweb/core.git"
  },
  "bugs": {
    "url": "https://github.com/qualweb/core/issues",
    "email": "qualweb@fc.ul.pt"
  },
  "author": {
    "name": "João Vicente",
    "email": "joao.vicente@campus.ul.pt"
  },
  "contributors": [
    {
      "name": "Carlos Duarte"
    },
    {
      "name": "Bruno Andrade"
    },
    {
      "name": "António Estriga"
    },
    {
      "name": "André Santos",
      "url": "https://arbsantos.github.io"
    }
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "ISC",
  "dependencies": {
    "@qualweb/crawler": "^0.3.5",
    "@qualweb/dom": "^0.2.1",
    "@qualweb/earl-reporter": "^0.4.1",
    "@qualweb/evaluation": "^0.3.0-alpha",
    "@qualweb/locale": "^0.1.1-alpha",
    "colors": "^1.4.0",
    "puppeteer": "^9.1.1",
    "puppeteer-cluster": "^0.22.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-adblocker": "^2.11.11",
    "puppeteer-extra-plugin-stealth": "^2.7.8"
  },
  "devDependencies": {
    "@qualweb/types": "^0.7.6-alpha",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sonarjs": "^0.7.0",
    "esm": "^3.2.25",
    "mocha": "^8.4.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
